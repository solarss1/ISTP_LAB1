@model CMSWebAppLab1.Models.Ticket

@{
    ViewData["Title"] = "Деталі";
}

<h1>Деталі</h1>

<div>
    <h4>Квиток</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Місце:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SeatNumber)
        </dd>
        <dt class = "col-sm-2">
            Час продажу:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SoldTime)
        </dd>
        <dt class = "col-sm-2">
            Сеанс:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Session.Id)
        </dd>
    </dl>
</div>
<p></p>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Редагувати</a> |
    <a asp-action="Index">Повернутися до списку</a>
</div>
